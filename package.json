{
  "name": "export-contract-generator",
  "version": "1.0.0",
  "description": "Web-based Export Contract Generator for automating commercial contract creation and management",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "set NODE_OPTIONS=--max_old_space_size=4096 && npm start --prefix client",
    "install-all": "npm install && npm install --prefix client",
    "build": "set NODE_OPTIONS=--max_old_space_size=4096 && npm run build --prefix client",
    "vercel-build": "npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "export",
    "contract",
    "generator",
    "commercial",
    "document"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "body-parser": "^1.20.2",
    "pdfkit": "^0.14.0",
    "docxtemplater": "^3.42.3",
    "pizzip": "^3.1.4",
    "number-to-words": "^1.2.4",
    "date-fns": "^3.0.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
